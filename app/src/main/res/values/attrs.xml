<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PaintBoard">
        <attr name="minStrokeWidth" format="float" />
        <attr name="maxStrokeWidth" format="float" />
        <attr name="currentColor" format="color" />
        <attr name="currentStrokeWidth" format="float" />
        <attr name="brushType" format="enum">
            <enum name="STROKE" value="0" />
            <enum name="RECTANGLE" value="1" />
            <enum name="CIRCLE" value="2" />
            <enum name="ERASER" value="3" />
        </attr>
    </declare-styleable>
</resources>
